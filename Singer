
public class Singer {
	
	String name;
	int age;
	double height;
	
	Singer(String name,int age,double height){
		
		this.name = name;
		this.age = age;
		this.height = height;
	}
	
	void sing(){
		System.out.println(this.name+" is singing");
	}
	
	void rickroll(){
		System.out.println(this.name+" is rickrolling");
	}

}
